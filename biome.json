{
	"$schema": "https://biomejs.dev/schemas/1.8.2/schema.json",
	"files": {
		"ignore": ["src/routeTree.gen.ts"]
	},
	"formatter": {
		"enabled": true
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "single"
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"organizeImports": {
		"enabled": true
	}
}
